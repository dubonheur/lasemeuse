

{% block body %}
    <h1>Supprimer la inscription</h1>
    <p>Voulez-vous vraiment supprimer cette inscription  ?</p>
    <p>{{ inscriptions }}</p>

    <form action="{{ path('inscription_supprimer', { 'id': inscriptions.id }) }}" method="post">
        <input type="hidden" name="_method" value="DELETE">
        <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ inscriptions.id) }}">
        <button type="submit" class="btn btn-danger">Supprimer</button>
    </form>

{% endblock %}